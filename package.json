{
  "name": "mitsuha",
  "version": "1.3.0-dev",
  "description": "Multi-purpose Discord bot.",
  "main": "dist/Mitsuha.js",
  "repository": "https://github.com/ayannw/mitsuha-project.git",
  "author": "Ayan <https://github.com/ayannw>",
  "license": "MIT",
  "private": false,
  "imports": {
    "#root/*": "./dist/*.js",
    "#lib/logger": "./dist/lib/logger.js",
    "#lib/MitsuhaClient": "./dist/lib/MitsuhaClient.js",
    "#lib/handlers/*": "./dist/lib/handlers/*.js",
    "#lib/utils/*": "./dist/lib/utils/*.js"
  },
  "type": "module",
  "scripts": {
    "start": "nodemon --es-module-specifier-resolution=node .",
    "build": "tsc -b src",
    "watch": "tsc -b src -W",
    "refresh": "rm -rf dist ; yarn build",
    "pretty": "prettier . -w",
    "lint": "eslint ./src/ --ext .ts --fix"
  },
  "dependencies": {
    "@sapphire/stopwatch": "^1.0.5",
    "colorette": "^1.2.2",
    "dayjs": "^1.10.4",
    "discord.js": "^12.5.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "prettier": "^2.2.1",
    "typescript": "^4.3.0-dev.20210303",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0"
  }
}
